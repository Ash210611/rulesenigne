<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.1.0">
    <localRepository>/var/maven/.m2/repository</localRepository>
    <servers>
        <server>
            <id>releases</id>
            <username>${userid}</username>
            <password>${apikey}</password>
        </server>
        <server>
            <id>snapshots</id>
            <username>${userid}</username>
            <password>${apikey}</password>
        </server>
    </servers>
    <mirrors>
        <mirror>
            <mirrorOf>*,!snapshot-repos,!plugins-release-repos,!plugins-snapshot-repos</mirrorOf>
            <name>maven-release-repos</name>
            <url>https://repo.sys.cigna.com/artifactory/maven-release-repos</url>
            <id>maven-release-repos</id>
        </mirror>
    </mirrors>
    <pluginGroups>
        <pluginGroup>com.cigna.maven.plugins</pluginGroup>
    </pluginGroups>
    <profiles>
        <profile>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>release-repos</id>
                    <name>maven-release-repos</name>
                    <url>https://repo.sys.cigna.com/artifactory/maven-release-repos</url>
                </repository>
                <repository>
                    <snapshots/>
                    <id>snapshot-repos</id>
                    <name>maven-snapshot-repos</name>
                    <url>https://repo.sys.cigna.com/artifactory/maven-snapshot-repos</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>plugins-release-repos</id>
                    <name>maven-plugin-repos</name>
                    <url>https://repo.sys.cigna.com/artifactory/maven-plugin-repos</url>
                </pluginRepository>
                <pluginRepository>
                    <snapshots/>
                    <id>plugins-snapshot-repos</id>
                    <name>maven-plugin-repos</name>
                    <url>https://repo.sys.cigna.com/artifactory/maven-plugin-repos</url>
                </pluginRepository>
            </pluginRepositories>
            <id>artifactory</id>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
</settings>
